{
  "name": "yanice",
  "version": "2.4.2",
  "description": "Yet another incremental command executor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "yanice": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "schema.json",
    "graph-display-template.html"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha -r ts-node/register \"**/*.spec.ts\"",
    "prepublishOnly": "npm test && npm run lint && npm run build"
  },
  "keywords": [
    "monorepo",
    "change detection"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abuob/yanice.git"
  },
  "author": "abuob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abuob/yanice/issues"
  },
  "homepage": "https://github.com/abuob/yanice#readme",
  "dependencies": {
    "ajv": "8.11.0",
    "async": "3.2.4",
    "minimatch": "5.1.0"
  },
  "devDependencies": {
    "@types/async": "3.2.13",
    "@types/chai": "4.3.1",
    "@types/minimatch": "3.0.5",
    "@types/mocha": "9.1.1",
    "@types/node": "18.0.0",
    "chai": "4.3.6",
    "mocha": "10.0.0",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.7.4"
  }
}
