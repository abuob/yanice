  "repository": {
    "type": "git",
    "url": "git+https://github.com/abuob/yanice.git"
  },
  "author": "abuob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abuob/yanice/issues"
  },
  "homepage": "https://github.com/abuob/yanice#readme",
  "dependencies": {
    "ajv": "^8.0.5",
    "async": "^3.2.0",
    "minimatch": "^3.0.4",
    "open": "^8.0.5"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/chai": "^4.2.16",
    "@types/minimatch": "^3.0.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "4.2.3"
  }
}
