{
  "name": "yanice",
  "version": "1.3.1",
  "description": "Yet another incremental command executor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "yanice": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "schema.json",
    "graph-display-template.html"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "mocha -r ts-node/register \"**/*.spec.ts\"",
    "prepublishOnly": "npm test && npm run lint && npm run build"
  },
  "keywords": [
    "monorepo",
    "change detection"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abuob/yanice.git"
  },
  "author": "abuob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abuob/yanice/issues"
  },
  "homepage": "https://github.com/abuob/yanice#readme",
  "dependencies": {
    "async": "^3.1.0",
    "minimatch": "^3.0.4",
    "open": "^7.0.2",
    "ajv": "^6.12.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.2",
    "@types/chai": "~4.2.0",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.6.9",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.8.2"
  }
}
