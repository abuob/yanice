  "author": "abuob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abuob/yanice/issues"
  },
  "homepage": "https://github.com/abuob/yanice#readme",
  "dependencies": {
    "ajv": "^6.12.2",
    "async": "^3.2.0",
    "minimatch": "^3.0.4",
    "open": "^7.0.3"
  },
  "devDependencies": {
    "@types/async": "^3.2.2",
    "@types/chai": "^4.2.11",
    "@types/minimatch": "^3.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.38",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3"
  }
}
